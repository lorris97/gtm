<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>inscription</servlet-name>
        <servlet-class>en.ensup.SERVLET.InscriptionSERVLET</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>liste</servlet-name>
        <servlet-class>en.ensup.SERVLET.RedirectListerEtudiantSERVLET</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>connection</servlet-name>
        <servlet-class>en.ensup.SERVLET.ConnectionSERVLET</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>creer</servlet-name>
        <servlet-class>en.ensup.SERVLET.RedirectInscriptionEtudiantSERVLET</servlet-class>
    </servlet>
    
    
    <servlet-mapping>
        <servlet-name>inscription</servlet-name>
        <url-pattern>/inscription</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>connection</servlet-name>
        <url-pattern>/connection</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>creer</servlet-name>
        <url-pattern>/creer</url-pattern>
    </servlet-mapping>
    
     <servlet-mapping>
        <servlet-name>liste</servlet-name>
        <url-pattern>/liste</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
